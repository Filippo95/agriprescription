<header class="clearfix">
      <div id="logo">
        <img src="
        data:image/png;base64,
        
        ">
      </div>
      <h1>Consigli trattamento </h1>
      <div id="company" class="clearfix">
        <div>Ulisse srl </div>
        <div>44042 Cento (FE),<br /> via alfieri, 10 </div>
        <div>p.iva 4532543253254325342</div>
      </div>
      <div id="project">
        <div><span>Azienda: </span>  <%=@companies.where(id: @prescription.id_company).first.name %></div>
        <div><span>Sede: </span> <%=@companies.where(id: @prescription.id_company).first.registred_office %></div>
        <div><span>Partita iva: </span> <%=@companies.where(id: @prescription.id_company).first.piva %></div>
        <div><span>Codice fiscale: </span> <%=@companies.where(id: @prescription.id_company).first.cf %></div>
        <div><span>Mail: </span> <%=@companies.where(id: @prescription.id_company).first.mail %></div>
        <br>
        <div><span>Data ricetta</span> <%= @prescription.date.strftime("%d/%m/%Y") %></div>
      </div>
    </header>
    <main>
      <table>
        <thead>
          <tr>
            <th class="service">Prodotto</th>
            <th>Dose (gr/Ha</th>
            <th>Quantità Totale</th>
       
          </tr>
        </thead>
        <tbody>
             <%= content_tag_for(:tr, @rows) do |prescription_row| %>
            
            <td class="service"><%= @products.where(id: prescription_row.id_product).first.nome %></td>
            <td class="unit"><%= prescription_row.dose_per_ha %> gr/Ha</td>
            <td class="total"><%= prescription_row.dose_per_ha*@lands.where(id: @prescription.land).first.ha %> gr</td>
      <% end %>
         
         
        </tbody>
      </table>
      <div id="notices">
        <div>NOTE:</div>
        <div class="notice"> <%= @prescription.note %></div>
      </div>
    </main>
    <footer>
      Agriprescription
    </footer>
