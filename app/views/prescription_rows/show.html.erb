<div class="page-header">
  <%= link_to prescription_rows_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_prescription_row_path(@prescription_row), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show prescription_row</h1>
</div>

<dl class="dl-horizontal">
  <dt>Ricetta:</dt>
  <dd><%= @prescription_row.id_prescription %></dd>

  <dt>Prodotto:</dt>
  <dd><%= @products.where(id: @prescription_row.id_product).first.nome %></dd>

  <dt>Dose per ha:</dt>
  <dd><%= @prescription_row.dose_per_ha %></dd>

</dl>
